alias ll='ls -alF'

alias be='bundle exec'

alias gs='git status'
alias gl='git lg'
alias pull='git pull'
alias push='git push'
alias commit='git commit'

alias compress='tar -zcvf $1'
alias extract='tar -zxvf $1'

alias rm-swp='rm -rf ~/.vim/swap/*.swp'

alias vundle-install='vim +BundleInstall +qall'

function cdp()  { cd ~/projetos/$@; }
function cdpo() { cd ~/projetos/oss/$@; }

alias tmux='tmux -2'
